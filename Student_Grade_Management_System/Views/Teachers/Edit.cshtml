@model Student_Grade_Management_System.Models.Teacher

@{
    ViewData["Title"] = "Redaguoti mokytoją";
}

<h2>Redaguoti mokytoją</h2>

<form asp-action="Edit">
    <div class="form-group">
        <label asp-for="Username" class="control-label"></label>
        <input asp-for="Username" class="form-control" readonly />
    </div>
    <div class="form-group">
        <label asp-for="Password" class="control-label"></label>
        <input asp-for="Password" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Name" class="control-label"></label>
        <input asp-for="Name" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Surname" class="control-label"></label>
        <input asp-for="Surname" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="SSN" class="control-label"></label>
        <input asp-for="SSN" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Email" class="control-label"></label>
        <input asp-for="Email" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber" class="control-label"></label>
        <input asp-for="PhoneNumber" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Street" class="control-label"></label>
        <input asp-for="Street" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="City" class="control-label"></label>
        <input asp-for="City" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Building" class="control-label"></label>
        <input asp-for="Building" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Apartment" class="control-label"></label>
        <input asp-for="Apartment" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Išsaugoti</button>
</form>
<a asp-action="Index" class="btn btn-secondary">Atgal į sąrašą</a>